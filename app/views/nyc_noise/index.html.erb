<script>
  $(function() {

    function dropdownLabels() {
      $('#panel > .dropup > ul').append('<li>' + 'All Data' + '</li>');
      <% @complaint_types.each do |type| %>
        $('#panel > .dropup > ul').append('<li>' + '<%= type %>' + '</li>');
      <% end %>
    }

    dropdownLabels();

    $.ajax({
            url: '/initialize',
            method: 'GET',
            data: {descriptor: "All_Data"},
            dataType: 'script'
        });

    });

    
</script>
